{
  "compilerOptions": {
    "types": ["node"],
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "lib": ["es2015", "dom"],
    "moduleResolution": "node"
  },
  "include": ["./src/**/*", "src/new.ts"]
}
